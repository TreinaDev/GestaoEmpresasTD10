<div class="card border-primary">
    <div class="card-body">
      <h4 class="card-title">Nova Empresa</h4>
      <%= form_with model: @company  do |f| %>
        <div class="mb-3">
          <%= f.label :brand_name, class: 'form-label' %>
          <%= f.text_field :brand_name, class: 'form-control'%>
        </div>
        <div class="mb-3">
          <%= f.label :corporate_name, class: 'form-label' %>
          <%= f.text_field :corporate_name, class: 'form-control'%>
        </div>
        <div class="mb-3">
          <%= f.label :registration_number, class: 'form-label' %>
          <%= f.text_field :registration_number, class: 'form-control'%>
        </div>
        <div class="mb-3">
          <%= f.label :address, class: 'form-label' %>
          <%= f.text_field :address, class: 'form-control'%>
        </div>
        <div class="mb-3">
          <%= f.label :phone_number, class: 'form-label' %>
          <%= f.text_field :phone_number, class: 'form-control'%>
        </div>
        <div class="mb-3">
          <%= f.label :email, class: 'form-label' %>
          <%= f.text_field :email, class: 'form-control'%>
        </div>
        <div class="mb-3">
          <%= f.label :domain, class: 'form-label' %>
          <%= f.text_field :domain, class: 'form-control'%>
        </div>
        <div class="mb-3">
          <%= f.label :logo, class: 'form-label' %>
          <%= f.file_field :logo, class: 'form-control'%>
        </div>
        <%= f.submit t('.save')%>
      <% end %>
    </div>
</div>
